@example
$InstallationDirectory = @@@{
  "AlteraQuartus" =   "C:\Altera\16.0";
  "IntelQuartus" =    "C:\Altera\20.1";
  "LatticeDiamond" =  "C:\Lattice\Diamond\3.10_x64";
  "XilinxISE" =       "C:\Xilinx\14.7\ISE_DS";
  "XilinxVivado" =    "C:\Xilinx\Vivado\2020.2";
